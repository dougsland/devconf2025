cid=$(podman ps -q --filter name=pw1)
host_pid=$(podman inspect "$cid" --format '{{.State.Pid}}')
echo "Program pw1 is PID: $host_pid"

gcore -o /tmp/core "$host_pid"

# find the any default string
strings "/tmp/core.$host_pid" | grep -i -e "REDHAT" -e "password" -e "devconf2025" || echo "Not found"
